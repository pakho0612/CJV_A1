
  {
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build" 
        }
      }
    ],
    "routes": [
      {
        "src": "/moviesTv/(.*)",
        "dest": "server/server.js"
      },
      {
        "src": "/heroMovies(.*)",
        "dest": "server/server.js"
      }
    ]
  }
