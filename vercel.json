
  {
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "rewrites": [
      
      {
        "source": "/(.*)",
        "destination": "index.js"
      },
      {
        "source": "/moviesTv(.*)",
        "destination": "server/server.js"
      },
      {
        "source": "/heroMovies",
        "destination": "server/server.js"
      }
    ]
  }